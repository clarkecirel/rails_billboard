<h1 class='ui header'>Billboard Charts</h1>

<%= link_to 'New Chart', new_billboard_path %>
<hr />

<% @billboards.each do |billboard| %>
  
<h1> <%= billboard.chart %></h1>
  
<%= link_to 'Show', billboard_path(billboard) %> |
<%= link_to 'Edit', edit_billboard_path(billboard)%> |
<%= link_to 'Delete', billboard_path(billboard), method: :delete %>

<% end %>
  

